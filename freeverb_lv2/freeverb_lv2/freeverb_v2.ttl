			rdfs:label "1" ;
			rdf:value 1.0
		] , [
			rdfs:label "1/2" ;
			rdf:value 0.5
		] , [
			rdfs:label "0" ;
			rdf:value 0.0
		] 
	], [

		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 3 ;
