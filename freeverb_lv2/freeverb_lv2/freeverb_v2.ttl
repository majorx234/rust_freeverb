@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://example.org/freever_lv2_rust>
	a lv2:Plugin ,
	    lv2:FreeverbPlugin ;

	lv2:project <http://example.org> ;

	doap:name "Freeverb plugin in Rust" ;
	doap:license <http://opensource.org/licenses/isc> ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port [

		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 0 ;
		lv2:symbol "dampening" ;
		lv2:name "Dampening" ;

		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;

		units:unit units:db ;
		lv2:scalePoint [
			rdfs:label "1" ;
			rdf:value 1.0
		] , [
			rdfs:label "1/2" ;
			rdf:value 0.5
		] , [
			rdfs:label "0" ;
			rdf:value 0.0
		] 
	] , [

		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 1 ;
